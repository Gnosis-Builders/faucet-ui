{
    "name": "faucet-gui",
    "version": "0.1.0",
    "private": true,
    "homepage": " https://gnosisfaucet.com",
    "dependencies": {
        "@emotion/react": "^11.7.0",
        "@emotion/styled": "^11.6.0",
        "@hcaptcha/react-hcaptcha": "^1.4.4",
        "@mui/icons-material": "^5.2.0",
        "@mui/material": "^5.2.2",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.11.45",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "query-string": "^7.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-ga": "^3.3.1",
        "react-scripts": "5.0.1",
        "react-toastify": "^9.0.7",
        "sass": "^1.53.0",
        "typescript": "^4.7.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "linter": "yarn lint:check && yarn format:check",
        "linter:fix": "yarn lint:fix && yarn format:fix",
        "lint:check": "npx eslint src/ --ext .js,.jsx,.ts,.tsx",
        "format:check": "npx prettier --check src/**/*.ts*",
        "format:fix": "npx prettier --ignore-path .gitignore --check --write src/**/*.ts*",
        "lint:fix": "npx eslint src/ --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix",
        "start": "react-scripts start",
        "build": "yarn linter && react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "engines": {
        "yarn": ">= 1.22.19"
    }
}
